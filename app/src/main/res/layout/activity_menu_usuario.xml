<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E6E6E6"
    tools:context=".MenuUsuarioActivity">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E6E6E6"
    tools:context=".MenuUsuarioActivity">

    <TextView
        android:id="@+id/label_Titulo_MenuUsuarioActivity"
        android:layout_width="415dp"
        android:layout_height="86dp"
        android:background="#E6F8CC32"
        android:fontFamily="@font/calistoga"
        android:gravity="center"
        android:text="ParKing"
        android:textAlignment="center"
        android:textColor="#0C0C0C"
        android:textSize="48sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/boton_Buscar_MenuUsuarioActivity"
        android:layout_width="320dp"
        android:layout_height="70dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="50dp"
        android:text="Buscar aparcamiento"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_Titulo_MenuUsuarioActivity" />

    <Button
        android:id="@+id/boton_Vender_MenuUsuarioActivity"
        android:layout_width="320dp"
        android:layout_height="70dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="50dp"
        android:text="Vender mi plaza"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton_Buscar_MenuUsuarioActivity" />

    <Button
        android:id="@+id/boton_MisReservas_MenuUsuarioActivity"
        android:layout_width="320dp"
        android:layout_height="70dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="50dp"
        android:text="Mis reservas"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton_Vender_MenuUsuarioActivity" />

    <Button
        android:id="@+id/boton_Salir_MenuUsuarioActivity"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="20dp"
        android:text="Salir"
        app:backgroundTint="#028CCA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/Boton_Ayuda_MenuUsuarioActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:text="Ayuda"
        app:backgroundTint="#028CCA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/boton_Perfil_MenuUsuarioActivity"
        app:layout_constraintStart_toEndOf="@+id/boton_Salir_MenuUsuarioActivity" />

    <Button
        android:id="@+id/boton_Perfil_MenuUsuarioActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="20dp"
        android:text="Mi espacio"
        app:backgroundTint="#028CCA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/botonImagen_Anuncio_MenuUsuarioActivity"
        android:layout_width="253dp"
        android:layout_height="204dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="100dp"
        app:layout_constraintBottom_toTopOf="@+id/Boton_Ayuda_MenuUsuarioActivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton_MisReservas_MenuUsuarioActivity"
        app:layout_constraintVertical_bias="0.454"
        tools:srcCompat="@drawable/img_5" />

</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>